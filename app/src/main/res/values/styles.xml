<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">

        <!-- Customize your theme here. -->
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
    </style>

    <declare-styleable name="LineCharView">
        <attr name="lv_ratiogap" format="integer" />
        <attr name="lv_ratioSmoothLine" format="integer" />
        <attr name="lv_densityscreen" format="integer" />
        <attr name="lv_color_line_upload" format="color" />
        <attr name="lv_color_line_download" format="color" />
    </declare-styleable>
</resources>